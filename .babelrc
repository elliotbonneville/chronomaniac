{
	"presets": [
		"es2015",
		"react",
		"stage-0"
	],
	"plugins": [
		[
			"babel-root-import", {
				"rootPathSuffix": "src/js"
			}
		],
		[
			"babel-plugin-module-alias", [
				{ "src": "./node_modules/paper/dist/paper-core", "expose": "paper" }
			]
		]
	],
	"env": {
		"test": {
			"plugins": [
				[
					"babel-plugin-module-alias", [
						{ "src": "./test", "expose": "test" }
					]
				]
			]
		}
	}
}
